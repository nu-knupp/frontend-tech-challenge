{
  "name": "frontend-tech-challenge",
  "version": "0.1.1",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm run dev:prepare-server-json\" \"pnpm run dev:json\" \"pnpm run dev:banking\" \"pnpm run dev:dashboard\"",
    "dev:script": "./scripts/dev.sh",
    "dev:banking": "pnpm --filter banking run dev",
    "dev:dashboard": "pnpm --filter dashboard run dev",
    "dev:prepare-server-json": "node scripts/initServerJson.js",
    "dev:json": "json-server --watch db/server.json --port 3001",
    "build": "turbo build",
    "build:banking": "pnpm --filter banking run build",
    "build:dashboard": "pnpm --filter dashboard run build",
    "install:all": "pnpm install",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules apps/*/node_modules packages/*/node_modules"
  },
  "type": "module",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.2",
    "@mui/system": "^7.1.1",
    "@rschpdr/react-money-input": "^0.1.3",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "next": "15.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-number-format": "^5.4.4",
    "recharts": "^2.15.4",
    "uuid": "^11.1.0",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.0",
    "json-server": "^0.17.3",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "turbo": "^2.0.0",
    "typescript": "^5.8.3",
    "zod": "^3.25.4"
  }
}
